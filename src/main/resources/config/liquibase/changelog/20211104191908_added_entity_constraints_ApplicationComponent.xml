                                 baseTableName="rel_application_component__categories"
                                 constraintName="fk_rel_application_component__categories__categories_id"
                                 referencedColumnNames="id"
                                 referencedTableName="application_category"/>

        <addForeignKeyConstraint baseColumnNames="application_component_id"
                                 baseTableName="rel_application_component__technologies"
                                 constraintName="fk_rel_application_component__t__application_component_67_id"
                                 referencedColumnNames="id"
                                 referencedTableName="application_component"/>

        <addForeignKeyConstraint baseColumnNames="technologies_id"
